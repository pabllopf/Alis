<?xml version="1.0" encoding="utf-8"?>
<Project>
    <!-- Select projects to disable parallel execution -->
    <ItemGroup Condition="$(AssemblyName.Contains('Graphic'))">
        <Content  
            Include="$(SolutionDir).config/xunit.runner.json" 
            LinkBase="." 
            Pack="true" 
            Visible="true" 
            PackagePath=".">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </Content>
    </ItemGroup>
</Project>