          $(SolutionDir)4_Operation/**/generator/**/*.Generator.csproj;
          $(SolutionDir)5_Declaration/**/generator/**/*.Generator.csproj;
          $(SolutionDir)6_Ideation/**/generator/**/*.Generator.csproj;"
