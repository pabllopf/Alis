            CopyToOutputDirectory="PreserveNewest"
            Visible="false"/>

        <None
            Condition="$(AssemblyName.Contains('Generator')) == false AND $(AssemblyName.Contains('Extension')) == false"
            Include="$(SolutionDir)**/**/generator/bin/Release/netstandard2.0/*Generator.pdb"
            Pack="false"
            TargetPath="../../analyzers/dotnet/cs/%(Filename)%(Extension)"
            Link="analyzers/dotnet/cs/%(Filename)%(Extension)"
            CopyToPublishDirectory="PreserveNewest"
            CopyToOutputDirectory="PreserveNewest"
            Visible="false"/>

        <None
            Condition="$(AssemblyName.Contains('Generator')) == false AND $(AssemblyName.Contains('Extension')) == false"
            Include="$(SolutionDir)**/**/generator/bin/Release/netstandard2.0/*Generator.deps.json"
            Pack="false"
            TargetPath="../../analyzers/dotnet/cs/%(Filename)%(Extension)"
            Link="analyzers/dotnet/cs/%(Filename)%(Extension)"
            CopyToPublishDirectory="PreserveNewest"
            CopyToOutputDirectory="PreserveNewest"
            Visible="false"/>

        <Content
            Condition="$(AssemblyName.Contains('Generator')) == false AND $(AssemblyName.Contains('Extension')) == false"
            Include="$(ProjectDir)bin/Release/analyzers/dotnet/cs/*.dll" Visible="false" PackagePath="analyzers/dotnet/cs/"/>

        <Content
            Condition="$(AssemblyName.Contains('Generator')) == false AND $(AssemblyName.Contains('Extension')) == false"
            Include="$(ProjectDir)bin/Release/analyzers/dotnet/cs/*.pdb" Visible="false" PackagePath="analyzers/dotnet/cs/"/>

        <Content
