<Project>
    <PropertyGroup Condition="$(AssemblyName.Contains('Test')) == true">
        <IsPackable>false</IsPackable>
    </PropertyGroup>

    <PropertyGroup Condition="$(AssemblyName.Contains('Sample')) == true">
        <IsPackable>false</IsPackable>
    </PropertyGroup>

  <PropertyGroup Condition="$(AssemblyName.Contains('Template')) == true">
    <IsPackable>false</IsPackable>
  </PropertyGroup>

  <PropertyGroup Condition="$(AssemblyName.Contains('App')) == true">
    <IsPackable>false</IsPackable>
  </PropertyGroup>

    <PropertyGroup Condition="$(AssemblyName.Contains('Test')) == false AND $(AssemblyName.Contains('Sample')) == false AND $(AssemblyName.Contains('Template')) == false AND $(AssemblyName.Contains('App')) == false">
        <IsPackable>true</IsPackable>
    </PropertyGroup>
</Project>
