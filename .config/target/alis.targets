            File="$(MSBuildProjectDirectory)/Interop.cs"
            Lines=
            '
                using System%3b
                using System.Runtime.InteropServices.JavaScript%3b
                
                namespace $(RootNamespace)
                {
                    internal static partial class Interop
                    {
                        [JSImport("initialize", "main.js")]
