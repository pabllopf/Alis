<Project>
  <PropertyGroup Condition="
        $(AssemblyName.Contains('Test')) == true OR
        $(AssemblyName.Contains('App')) == true OR
        $(AssemblyName.Contains('Template')) == true OR
        $(AssemblyName.Contains('Sample')) == true
        ">
        <TargetFramework1>$(TargetFramework1)</TargetFramework1>
        <TargetFramework2>net6.0</TargetFramework2>
    </PropertyGroup>
    
    <PropertyGroup Condition="
        $(AssemblyName.Contains('Test')) == false AND 
        $(AssemblyName.Contains('App')) == false AND
        $(AssemblyName.Contains('Template')) == false AND
        $(AssemblyName.Contains('Sample')) == false
        ">
        <TargetFrameworkVersion>1.3</TargetFrameworkVersion>
        <TargetFramework1>netstandard2.0</TargetFramework1>
        <TargetFramework2>netstandard2.1</TargetFramework2>
    </PropertyGroup>
</Project>