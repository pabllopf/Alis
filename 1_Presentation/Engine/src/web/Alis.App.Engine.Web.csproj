<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="9.0.6" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="9.0.6" PrivateAssets="all" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\imgui\" />
  </ItemGroup>

  <ItemGroup>
    <_ContentIncludedByDefault Remove="wwwroot\src\imgui_bridge.ts" />
    <_ContentIncludedByDefault Remove="wwwroot\src\imgui_demo.ts" />
    <_ContentIncludedByDefault Remove="wwwroot\src\imgui_impl.ts" />
    <_ContentIncludedByDefault Remove="wwwroot\src\imgui_memory_editor.ts" />
    <_ContentIncludedByDefault Remove="wwwroot\src\main.ts" />
  </ItemGroup>

</Project>
