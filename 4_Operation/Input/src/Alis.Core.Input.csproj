<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="$(SolutionDir).config/Config.props"/>
    <PropertyGroup>
        <TargetFramework>$(TargetFramework2)</TargetFramework>
    </PropertyGroup>
    <ItemGroup>
        <Folder Include="Resources\linux-arm64"/>
    </ItemGroup>
    <ItemGroup>
        <EmbeddedResource Update="Properties\NativeInput.resx">
            <Generator>ResXFileCodeGenerator</Generator>
            <LastGenOutput>NativeInput.Designer.cs</LastGenOutput>
        </EmbeddedResource>
    </ItemGroup>
    <ItemGroup>
        <Compile Update="Properties\NativeInput.Designer.cs">
            <DesignTime>True</DesignTime>
            <AutoGen>True</AutoGen>
            <DependentUpon>NativeInput.resx</DependentUpon>
        </Compile>
    </ItemGroup>
</Project>