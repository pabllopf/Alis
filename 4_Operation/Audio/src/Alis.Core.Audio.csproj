<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="$(SolutionDir).config/Config.props"/>
    <PropertyGroup>
        <TargetFramework>$(TargetFramework2)</TargetFramework>
    </PropertyGroup>
    <ItemGroup>
        <Compile Update="Properties\NativeAudio.Designer.cs">
            <DesignTime>True</DesignTime>
            <AutoGen>True</AutoGen>
            <DependentUpon>NativeAudio.resx</DependentUpon>
        </Compile>
    </ItemGroup>
    <ItemGroup>
        <EmbeddedResource Update="Properties\NativeAudio.resx">
            <Generator>ResXFileCodeGenerator</Generator>
            <LastGenOutput>NativeAudio.Designer.cs</LastGenOutput>
        </EmbeddedResource>
    </ItemGroup>
</Project>